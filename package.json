{
  "name": "react-starter-app",
  "version": "1.0.0",
  "description": "React Starter Application",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "serve --single build",
    "watch": "webpack --watch --display-error-details",
    "build:clean": "rimraf build coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "lint": "eslint src/ --ignore-path .eslintignore",
    "lint:fix": "eslint src/ --fix --ignore-path .eslintignore",
    "stylelint-check": "stylelint-config-prettier-check",
    "stylelint": "stylelint 'src/**/*.{css,scss,less}'",
    "stylelint:fix": "stylelint --fix 'src/**/*.{css,scss,less}'",
    "prettier": "prettier --write --single-quote src/**/*.js",
    "precommit": "lint-staged",
    "generate": "plop --plopfile generators/index.js"
  },
  "lint-staged": {
    "*.{js,jsx,md,css,html}": [
      "prettier --single-quote --write",
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamedsamara/react-starter-app.git"
  },
  "keywords": [
    "react",
    "redux",
    "redux-thunk",
    "react-intl",
    "jest",
    "enzyme",
    "sass",
    "less"
  ],
  "author": "Mohamed Samara (https://github.com/mohamedsamara)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohamedsamara/react-starter-app/issues"
  },
  "homepage": "https://github.com/mohamedsamara/react-starter-app#readme",
